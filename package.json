{
  "name": "embed-images-in-markdown",
  "displayName": "Embed images in markdown",
  "description": "Drag and drop an image onto the markdown to embed it as a DataUrl.",
  "version": "0.0.4",
  "publisher": "hidao80",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon256.png",
  "license": "MIT",
  "keywords": [
    "drag and drop",
    "drag",
    "embed images",
    "embed",
    "image",
    "markdown"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hidao80/embed-images-in-markdown.git"
  },
  "scripts": {
    "build": "npm run compile && vsce package",
    "compile": "tsc -p ./",
    "watch": "npm run compile",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/uuid": "^9.0.0",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.3",
    "uuid": "^9.0.0"
  }
}
